{
  "name": "secret-hitler-pants",
  "description": "Minimalistic Secret Hitler web server",
  "keywords": [
    "secret-hitler",
    "game",
    "party"
  ],
  "version": "0.2.1",
  "private": true,
  "license": "cc-by-nc-sa-4.0",
  "scripts": {
    "build": "rimraf .cache dist && tsc -p tsconfig.server.json && parcel build ./app/index.html --public-url /-/ --out-dir dist/app",
    "start": "node dist/server",
    "start:dev": "cross-env NODE_ENV=development ts-node-dev src",
    "test": "npm run lint && jest",
    "test:watch": "jest --watch",
    "lint": "eslint \"src/**/*.ts\" \"app/**/*.ts\" \"app/**/*.tsx\" && tsc --noEmit"
  },
  "engines": {
    "node": "10.x"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/express-session": "^1.17.0",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@types/parcel-bundler": "^1.12.1",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/seedrandom": "^2.4.28",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.6.0",
    "eslint-plugin-react": "^7.20.5",
    "jest": "^26.3.0",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2",
    "sass": "^1.26.10",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "seedrandom": "^3.0.5",
    "ws": "^7.3.1"
  }
}
